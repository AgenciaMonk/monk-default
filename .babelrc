{
  "presets": [
    ["env", { "modules": false }],
    ["es2015", { "modules": false }],
    "stage-2",
    "vue-app"
  ],
  "plugins": ["transform-runtime", [
    "component", [{
      "libraryName": "element-ui",
      "styleLibraryName": "~theme"
    }]
  ]],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
